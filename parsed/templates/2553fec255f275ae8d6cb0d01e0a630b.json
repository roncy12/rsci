{"components/custom/brands-grid":"<ul class=\"brandGrid productGrid--maxCol{{ columns }}\">\n    {{#each brands}}\n        <li class=\"brand\">\n            <article class=\"card {{#if alternate}}card--alternate{{/if}}\">\n                <figure class=\"card-figure\">\n                    <a class=\"image-link\" href=\"{{url}}\">\n                        <img class=\"card-image\" src=\"{{getImage image 'gallery' (cdn ../theme_settings.default_image_brand)}}\" alt=\"{{name}}\" title=\"{{name}}\">\n                    </a>\n                </figure>\n                <div class=\"card-body\">\n                    <h4 class=\"card-title\">\n                        <a class=\"name-link\" href=\"{{url}}\">{{name}}</a>\n                    </h4>\n                </div>\n            </article>\n        </li>\n    {{/each}}\n</ul>\n"}
