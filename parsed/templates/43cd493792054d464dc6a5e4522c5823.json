{"components/custom/account-dropdown":"{{#if settings.account_creation_enabled}}\n<ul class=\"account-dropdown enabled\">\n    <li class=\"navUser-item navUser-item--account\">\n      {{#if customer}}\n            <a class=\"navUser-action account signout-list\" href=\"\"><svg><use xlink:href=\"#icon-profile\" /></svg></a>\n            <div class=\"dropdown-menu signout-list\" id=\"account-dropdown-signout-list\">\n              <h2 class=\"container-header\">Account Info</h2>\n                {{> components/custom/account-dropdown-info}}\n              <div class=\"or-text signout-list\"> <hr class=\"left\"/> <span>or</span> <hr class=\"right\" /></div>\n              <a class=\"button button--primary signin-register logout-button\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            </div>\n      {{else}}\n            <a class=\"navUser-action account signin-register\" href=\"\"><svg><use xlink:href=\"#icon-profile\" /></svg></a>\n            <div class=\"dropdown-menu signin-register\" id=\"account-dropdown-signin-register\">\n              <h2 class=\"container-header\">{{lang 'common.login'}}</h2>\n              <form class=\"login-form-dropdown form signin-register\" action=\"/login.php?action=check_login\" method=\"post\">\n                <div class=\"form-field form-field--input form-field--inputEmail signin-register\">\n                  <input class=\"form-input signin-register\" name=\"login_email\" placeholder=\"email\" id=\"login_email\" type=\"email\">\n                  <span style=\"display: none;\"></span></div>\n                  <div class=\"form-field form-field--input form-field--inputPassword signin-register\">\n                    <input class=\"form-input signin-register\" placeholder=\"password\" id=\"login_pass\" type=\"password\" name=\"login_pass\">\n                    <span style=\"display: none;\"></span></div>\n                    <div class=\"form-actions signin-register\">\n                      <input type=\"submit\" class=\"button button--primary signin-register\" value=\"Login\">\n                      <a class=\"forgot-password signin-register\" href=\"/login.php?action=reset_password\">Forgot your password?</a>\n                    </div>\n              </form>\n\n              <div class=\"or-text signin-register\"> <hr class=\"left\"/> <span>or</span> <hr class=\"right\" /></div>\n              <a class=\"navUser-action create button button--primary register signin-register\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n            </div>\n\n      {{/if}}\n     </li>\n{{/if}}\n\n<li class=\"navUser-item navUser-item--cart desktop\">\n  <a\n  class=\"navUser-action\"\n  data-cart-preview\n  data-dropdown=\"cart-preview-dropdown\"\n  data-options=\"align:right\"\n  href=\"{{urls.cart}}\">\n  <span class=\"navUser-item-cartLabel\"><svg><use xlink:href=\"#icon-cart\" /></svg></span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\n  </a>\n\n<div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n</li>\n</ul>\n","components/custom/account-dropdown-info":"<nav class=\"navBar navBar--sub navBar--account signout-list\">\n    <ul class=\"navBar-section signout-list\">\n        {{#if account_page '===' 'orders'}}\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.orders'}}</li>\n        {{else}}\n            <li class=\"navBar-item signout-list\">\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n            </li>\n        {{/if}}\n        {{#if settings.returns_enabled}}\n            {{#if account_page '===' 'returns'}}\n                <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.returns'}}</li>\n            {{else}}\n                <li class=\"navBar-item signout-list\">\n                    <a class=\"navBar-action signout-list\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                </li>\n            {{/if}}\n        {{/if}}\n        {{#if account_page '===' 'messages'}}\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</li>\n        {{else}}\n            <li class=\"navBar-item signout-list\">\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.inbox}}\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</a>\n            </li>\n        {{/if}}\n        {{#if account_page '===' 'addresses'}}\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.addresses'}}</li>\n        {{else}}\n            <li class=\"navBar-item signout-list\">\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\n            </li>\n        {{/if}}\n        {{#if settings.show_wishlist}}\n            {{#if account_page '===' 'wishlists'}}\n                <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</li>\n            {{else}}\n                <li class=\"navBar-item signout-list\">\n                    <a class=\"navBar-action signout-list\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</a>\n                </li>\n            {{/if}}\n        {{/if}}\n        {{#if account_page '===' 'recent_items'}}\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.recently_viewed'}}</li>\n        {{else}}\n            <li class=\"navBar-item signout-list\">\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n            </li>\n        {{/if}}\n        {{#if account_page '===' 'settings'}}\n            <li class=\"navBar-item is-active signout-list\">{{lang 'account.nav.settings'}}</li>\n        {{else}}\n            <li class=\"navBar-item signout-list\">\n                <a class=\"navBar-action signout-list\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n"}
