{"components/common/mega-nav-pages":"\n            <h2 class=\"container-header shop-by by-page\">Pages</h2>\n            {{#each pages}}\n                <li class=\"mega-pages sideTopLevel\">\n                    {{> components/common/mega-navigation-page-list}}\n                </li>\n            {{/each}}\n          \n","components/common/mega-navigation-page-list":"{{#if children}}\n<a class=\"navPages-action navPages-action-depth-max has-subMenu is-root topLevel\" href=\"{{url}}\">\n    {{name}} <i class=\"fa fa-chevron-down main-nav\" aria-hidden=\"true\"></i>\n</a>\n\n<div class=\"navPage-subMenu navPage-subMenu-horizontal flyout second-level no-images navPages-flyout\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n  <div class=\"category-title\">\n    <h2><a class=\"category-title\" href=\"{{url}}\">{{name}}</a></h2>\n  </div>\n    <ul class=\"navPage-subMenu-list all-second-level-plus-container flyout-page-wrapper\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a\n                        class=\"navPage-subMenu-action navPages-action has-subMenu\"\n                        href=\"{{url}}\"\n                        data-collapsible=\"navPages-{{id}}\"\n                        data-collapsible-disabled-breakpoint=\"medium\"\n                        data-collapsible-disabled-state=\"open\"\n                        data-collapsible-enabled-state=\"closed\">\n                        {{name}} <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i>\n                    </a>\n                    <ul class=\"navPage-subMenu-list all-second-level-plus-container flyout-page-wrapper\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n        <!-- END COMMENT OUT THE FOLLOWING SECTION TO ENABLE MAX NAV LEVELS -->\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n"}
