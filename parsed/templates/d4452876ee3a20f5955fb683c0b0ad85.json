{"components/common/mega-nav-pages":"\n            <h2 class=\"container-header shop-by by-page\">Pages</h2>\n            {{#each pages}}\n                <li class=\"mega-pages sideTopLevel\">\n                    {{> components/common/mega-navigation-page-list}}\n                </li>\n            {{/each}}\n          \n","components/common/mega-navigation-page-list":"{{#if children}}\n<a class=\"navPages-action navPages-action-depth-max has-subMenu is-root topLevel\" href=\"{{url}}\">\n    {{name}} <i class=\"fa fa-chevron-down main-nav\" aria-hidden=\"true\"></i>\n</a>\n\n<div class=\"navPage-subMenu navPage-subMenu-horizontal flyout second-level no-images navPages-flyout\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n  <div class=\"category-title\">\n    <h2><a class=\"category-title\" href=\"{{url}}\">{{name}}</a></h2>\n  </div>\n    <ul class=\"navPage-subMenu-list all-second-level-plus-container flyout-page-wrapper\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a\n                        class=\"navPage-subMenu-action navPages-action has-subMenu\"\n                        href=\"{{url}}\"\n                        data-collapsible=\"navPages-{{id}}\"\n                        data-collapsible-disabled-breakpoint=\"medium\"\n                        data-collapsible-disabled-state=\"open\"\n                        data-collapsible-enabled-state=\"closed\">\n                        {{name}} <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i>\n                    </a>\n                    <ul class=\"navPage-subMenu-list all-second-level-plus-container flyout-page-wrapper\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n        <!-- END COMMENT OUT THE FOLLOWING SECTION TO ENABLE MAX NAV LEVELS -->\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n","components/custom/mega-shop-nav/2-call-level-1-no-images-no-brands":"<div class=\"sideCategoryList desktop alternate no-images no-brands\" id=\"side-categories\" data-menu>\n    <div class=\"categories\">\n      <div class=\"alt-toggle\">\n        <h2 class=\"container-header btn\"><span class=\"down-arrrow burger-menu\"><i class=\"burger open\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-Menu\" /></svg></i><i class=\"menu close\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-close\" /></svg></i></span></h2>\n        <h2 class=\"container-header text\"><span class=\"mega-shop-text\">{{theme_settings.mega_nav_name}}</span><span class=\"mega-shop-text sticky\">{{theme_settings.sticky_mega_nav_name}}</span></h2>\n      </div>\n        <div class=\"alt-menu-container\">\n          {{#if theme_settings.show_pages_in_sticky_mega_nav '===' true}}\n          <div class=\"megaPages-container {{#if theme_settings.show_pages_in_sticky_mega_nav '===' true}}sticky-pages-visible {{/if}}\">\n            {{>components/common/mega-nav-pages}}\n          </div>\n          {{/if}}\n          <div id=\"page-wrapper\" class=\"drop-down\">\n          <div class=\"category-list\">\n            <h2 class=\"container-header shop-by by-category\">Shop by Category</h2>\n            <!-- <div class=\"scroller\"><span>SCROLL</span></div> -->\n            {{#each categories}}\n                <li class=\"navPages-item sideTopLevel\">\n                    {{> components/custom/mega-shop-nav/3-nav-list-no-images}}\n                </li>\n            {{/each}}\n            <div class=\"pagination-container\">\n              <a class=\"navPages-action previous\">\n                <i class=\"icon prev\" aria-hidden=\"true\">\n                  <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n                </i>\n              </a>\n              <span class=\"dots\"></span>\n              <a class=\"navPages-action next\">\n                <i class=\"icon next\" aria-hidden=\"true\">\n                  <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n                </i>\n              </a>\n            </div>\n            <hr class=\"sidenav\"/>\n          </div> <!-- category-list -->\n          </div>\n\n</div> <!-- sideCategoryList -->\n\n</div>\n</div> <!-- sideCategoryList -->\n","components/custom/mega-shop-nav/3-nav-list-no-images":"{{#if children}}\n    {{> components/custom/mega-shop-nav/4-flyout-no-images}}\n{{else}}\n    <a class=\"navPages-action no-images\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n","components/custom/mega-shop-nav/4-flyout-no-images":"{{#if children}}\n  <!-- TOP LEVEL MENU ITEMS WITH CHILDREN ONLY -->\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root topLevel\" href=\"{{url}}\">\n        {{name}}\n    </a>\n      {{> components/custom/mega-shop-nav/5-level-2-no-images}}\n{{else}}\n    <!-- TOP LEVEL MENU ITEMS WITHOUT CHILDREN ONLY -->\n    <a class=\"navPages-action\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n","components/custom/mega-shop-nav/5-level-2-no-images":"<div class=\"navPage-subMenu navPage-subMenu-horizontal flyout second-level no-images\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n  <div class=\"category-title\">\n    <h2><a class=\"category-title\" href=\"{{url}}\">{{name}}</a></h2>\n    <div class=\"flyout-pagination-container top\">\n      <a class=\"navPages-action previous\">\n        <i class=\"icon prev\" aria-hidden=\"true\">\n          <svg><use xlink:href=\"#icon-circle-left\" /></svg>\n        </i>\n      </a>\n      <!-- <span class=\"dots\"></span> -->\n      <a class=\"navPages-action next\">\n        <i class=\"icon next\" aria-hidden=\"true\">\n          <svg><use xlink:href=\"#icon-circle-right\" /></svg>\n        </i>\n      </a>\n    </div>\n  </div>\n\n    <ul class=\"navPage-subMenu-list all-second-level-plus-container flyout-page-wrapper\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max {{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child second-level-subMenu-plus\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        <span class=\"top-level-sub\">{{name}}</span>\n                    </a>\n                    <div class=\"subMenu-list third-level-plus\">\n                      {{> components/custom/mega-shop-nav/6-three-plus-list}}\n                    </div>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"top-level-sub\">{{name}}</span></a>\n                {{/if}}\n            </li>\n        {{/each}}\n        <div class=\"flyout-pagination-container bottom\">\n          <a class=\"navPages-action previous\">\n            <i class=\"icon prev\" aria-hidden=\"true\">\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n            </i>\n          </a>\n          <span class=\"dots\"></span>\n          <a class=\"navPages-action next\">\n            <i class=\"icon next\" aria-hidden=\"true\">\n              <svg><use xlink:href=\"#icon-chevron-left\" /></svg>\n            </i>\n          </a>\n        </div>\n    </ul>\n</div>\n","components/custom/mega-shop-nav/6-three-plus-list":"<div class=\"three-plus-submenu-container\" id=\"navPages-{{id}}\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max {{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}}\n                    </a>\n                    {{#if theme_settings '===' alt_nav_three_plus}}\n                      <div class=\"subMenu-list fourth-level-plus\">\n                        {{> components/custom/mega-shop-nav/6-three-plus-list}}\n                      </div>\n                    {{/if}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n        <li class=\"navPage-subMenu-item-parent view-all\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max view-all\" href=\"{{url}}\">View All</a>\n        </li>\n    </ul>\n</div>\n"}
