{"components/common/quick-search":"<div class=\"container\">\n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"u-hiddenVisually\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query \" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n                <div class=\"search-icon\"><svg><use xlink:href=\"#icon-search\" /></svg></div>\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"\">\n            </div>\n        </fieldset>\n        <div class=\"clear\"></div>\n    </form>\n    <a href=\"#\" class=\"reset quicksearch\" aria-label=\"reset search\" role=\"button\">\n      <span class=\"reset\" aria-hidden=\"true\">Reset Search</span>\n    </a>\n    <a href=\"#\" class=\"modal-close custom-quick-search\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n      <span aria-hidden=\"true\">&#215;</span>\n    </a>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\">\n    </section>\n</div>\n","components/custom/top-menu":"<div class=\"top-menu\">\n    <div class=\"content\">\n         <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\n            <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\n        </a>\n\n        <!-- OPTIONAL SHORT MESSAGE LINK -->\n        <!-- <div class=\"left\">\n          <a class=\"optional-message-link\" href=\"\">Put an optional message here.</a>\n        </div> -->\n\n        <div class=\"left phone-number\">\n            {{#if settings.phone_number}}\n            <a href=\"tel:{{settings.phone_number}}\">{{settings.phone_number}}</a>\n            {{/if}}\n        </div>\n\n        <!-- MOBILE LOGIN / LOGOUT -->\n        <div class=\"right top mobile\">\n        {{#if settings.account_creation_enabled}}\n          <div class=\"mobile-login\">\n                 <li class=\"mobile-account\">\n                     {{#if customer}}\n                         <a class=\"mobile-account-icon\" href=\"{{urls.account.index}}\"><svg><use xlink:href=\"#icon-profile\" /></svg></a>\n                         <a class=\"mobile-logout-text\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n                     {{else}}\n                         <a class=\"\" href=\"{{urls.auth.login}}\"><!-- <i class=\"fa fa-user\" aria-hidden=\"true\"></i> --><a class=\"\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>&nbsp;<span class=\"or-text\">or</span>&nbsp;<a class=\"\" href=\"{{urls.auth.create_account}}\">{{lang 'Sign Up'}}</a>\n                     {{/if}}\n                  </li>\n          </div>\n        {{/if}}\n\n        <!-- MOBILE CART -->\n        <div class=\"mobile-cart\">\n        <li class=\"navUser-item navUser-item--cart mobile\">\n            <a\n                class=\"navUser-action\"\n                data-cart-preview\n                data-dropdown=\"cart-preview-dropdown\"\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\">\n                <span class=\"navUser-item-cartLabel\"><svg><use xlink:href=\"#icon-cart\" /></svg></span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\n            </a>\n\n            <!-- <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div> -->\n        </li>\n      </div>\n    </div>\n\n        {{#if theme_settings.sticky_topMenu '===' true}}\n          <nav class=\"navPages sticky-nav search\">\n              <div class=\"navPages-quickSearch searchbar\">\n                  {{> components/common/quick-search}}\n              </div>\n          </nav>\n        {{/if}}\n\n        <div class=\"right account-links {{#if customer}} logout-visible{{/if}}\">\n          {{#if settings.gift_certificates_enabled}}\n                <div class=\"account-link gift-certificates\">\n                   <a href=\"{{urls.gift_certificate.purchase}}\">{{lang 'Gift Certificates'}}</a>\n                </div>\n           {{/if}}\n\n           {{#if settings.show_wishlist}}\n                <div class=\"account-link wishlist\">\n                  <a href=\"/wishlist.php\">Wishlist</a>\n                </div>\n          {{/if}}\n\n          <div class=\"account-link currency-converter\">\n            <ul class=\"navPages-list navPages-list--user\">\n                {{#if currency_selector.currencies.length '>' 1}}\n                    <li class=\"navPages-item\">\n                        <a class=\"navPages-action has-subMenu\" href=\"#\" data-collapsible=\"navPages-currency\" aria-controls=\"navPages-currency\" aria-expanded=\"false\">\n                            {{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use></svg></i>\n                        </a>\n                        <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\n                            <ul class=\"navPnavPagesage-subMenu-list\">\n                                {{#each currency_selector.currencies}}\n                                <li class=\"navPage-subMenu-item\">\n                                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                        {{#if is_active}}\n                                            <strong>{{name}}</strong>\n                                        {{else}}\n                                            {{name}}\n                                        {{/if}}\n                                    </a>\n                                </li>\n                                {{/each}}\n                            </ul>\n                        </div>\n                    </li>\n                {{/if}}\n            </ul>\n          </div>\n\n\n            {{#if settings.account_creation_enabled}}\n              <div class=\"mobile-login top-account-access\">\n                     <li class=\"\">\n                         {{#if customer}}\n                             <a class=\"\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n                         {{else}}\n                             <!-- <a class=\"\" href=\"{{urls.auth.login}}\"><a class=\"\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>&nbsp;<span class=\"\">or</span>&nbsp;<a class=\"\" href=\"{{urls.auth.create_account}}\">{{lang 'Sign Up'}}</a> -->\n                         {{/if}}\n                      </li>\n              </div>\n            {{/if}}\n\n\n        </div>\n        <div class=\"clear\"></div>\n    </div>\n</div>\n"}
